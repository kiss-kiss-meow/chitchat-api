  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kiss-kiss-meow/chitchat-api/issues"
  },
  "homepage": "https://github.com/kiss-kiss-meow/chitchat-api#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "boom": "^7.1.1",
    "config": "^1.26.2",
    "dotenv": "^4.0.0",
    "good": "^7.3.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.5.2",
    "hapi-auth-jwt2": "^7.3.0",
    "jsonwebtoken": "^8.1.0",
    "pg": "^7.4.1",
    "rotating-file-stream": "^1.3.2",
